{
    'plugins': [
        'react'
    ],
    'env': {
        'browser': true,
        'jquery': true,
        'node': true
    },
    'ecmaFeatures': {
        'blockBindings': true,
        'forOf': true,
        'jsx': true
    },
    'rules': {
        'semi': 2,
        'quotes': [2, 'single', 'avoid-escape'],
        'no-multi-spaces': 0,
        'no-unused-vars': [2, {'vars': 'local' }],
        'no-underscore-dangle': 0
    }
}